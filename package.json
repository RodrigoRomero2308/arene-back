{
  "name": "arene-back",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "^14 || ^16"
  },
  "scripts": {
    "prebuild": "rimraf dist && npx prisma generate",
    "render:build": "npm i && npm run build",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prestart": "npx prisma generate",
    "start": "nest start",
    "start:dev": "npx prisma generate && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "pretest": "npx prisma generate",
    "db-update": "npx prisma db push && npx prisma generate && npx prisma db seed",
    "db-reset": "npx prisma db push --force-reset && npx prisma generate && npx prisma db seed"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.7",
    "@nestjs/common": "^9.3.2",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.3.2",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/passport": "^9.0.1",
    "@nestjs/platform-express": "^9.3.2",
    "@prisma/client": "^4.3.1",
    "apollo-server-express": "^3.6.7",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-redis": "^6.1.3",
    "dotenv": "^16.0.0",
    "express-session": "^1.17.2",
    "googleapis": "^109.0.1",
    "graphql": "^15.8.0",
    "graphql-upload": "^13.0.0",
    "helmet": "^5.0.2",
    "ioredis": "^5.0.4",
    "mime-types": "^2.1.35",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@types/bcrypt": "^5.0.0",
    "@types/connect-redis": "^0.0.18",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/graphql-upload": "^8.0.4",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^16.0.0",
    "@types/passport-local": "^1.0.34",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "prisma": "^4.3.1",
    "prisma-dbml-generator": "^0.9.1",
    "prisma-nestjs-graphql": "^16.0.1",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "ts-node --transpile-only src/prisma/seed/seed.ts"
  },
  "overrides": {
    "qs": ">=6.9.7",
    "json5": ">=2.2.2",
    "undici": ">5.8.1"
  }
}
